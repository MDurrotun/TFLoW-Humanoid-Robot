            </geometry>
            <origin rpy="${roll} ${pitch} ${yaw}" xyz="${x} ${y} ${z}"/>
            <material name="white">
                <color rgba="0.8 0.8 0.8 1"/>
            </material>
        </visual>
        <collision>
          <geometry>
            <cylinder length="${le}" radius="${r}"/>
          </geometry>
          <origin rpy="${roll} ${pitch} ${yaw}" xyz="${x} ${y} ${z}"/>
        </collision>
        <inertial>
            <mass value="${le*r}"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </xacro:macro>

